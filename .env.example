# Database (use existing PostgreSQL or new one)
DATABASE_URL=postgres://postgres:yourpassword@postgres:5432/omnichannel
DB_USER=postgres
DB_PASSWORD=yourpassword
DB_NAME=omnichannel

# Meta API Configuration
META_ACCESS_TOKEN=your_meta_access_token
META_APP_SECRET=your_meta_app_secret
META_VERIFY_TOKEN=omnichannel_verify_token

# WhatsApp Business API
WHATSAPP_PHONE_ID=your_phone_number_id
WHATSAPP_BUSINESS_ID=your_whatsapp_business_account_id

# Instagram API
INSTAGRAM_ACCOUNT_ID=your_instagram_account_id

# n8n Integration (optional)
N8N_WEBHOOK_URL=http://localhost:5678/webhook/omnichannel
